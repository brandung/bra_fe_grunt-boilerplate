{
	"name": "download-builder",
	"description": "Get FE Modules and dependencies from GIT repositories",
	"author": "brandung GmbH & CO.KG (http://www.brandung.de)",
	"private": true,
	"dependencies": {
		"fe-workflow" : "https://github.com/brandung/bra_fe_grunt-boilerplate.git"
	},
	"devDependencies": {
		"bra-module-widget": "https://github.com/brandung/bra-module-widget.git#2.0.0"
  	},
	"ignore": [
		"hbp",
		".gitignore",
		"LICENSE",
		"README.md",
		"grunt/*.js",
		"grunt/systems"
	],
	"exportsOverride": {
		"comp-*": {
			"%%private%%/js/mod": "*.js",
			"%%private%%/sass/mod": "*.scss",
			"%%private%%/templates/mod": "*.html"
		},
		"bra-*": {
			"%%public%%/js/libs/bra": "."
		}
	}
}
